{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/Assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/Assets/(.*)",
      "dest": "public/Assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}